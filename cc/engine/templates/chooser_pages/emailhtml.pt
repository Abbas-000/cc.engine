<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="context/@@popup/page"
      i18n:domain="cc_org">
  <head>
    <title metal:fill-slot="title">Email Information</title>
  </head>

  <body>
    <metal:body fill-slot="body" >
      <p i18n:translate="license.link_emailed" >
        <span tal:omit-tag="" i18n:name="email" tal:content="request/to_email" />
      </p>

      <div align="center">
        <a href="javascript:window.close();" i18n:translate="util.Close_window" />
      </div>
    </metal:body>
  </body>
</html>

